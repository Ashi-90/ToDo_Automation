<suite name="Hotwire Web test Suite with Chrome Browser" verbose="1">
    <!-- This parameter will be passed to every test in this suite -->
    <parameter name="browserName" value="chrome"/>
    <parameter name="webURL" value="https://todomvc.com/examples/angular2/"/>
    <parameter name="headLess" value="false"/>
    <test name="Parameter Test one">
        <classes>
            <class name="com.aera.todoapp.test.ToDoAppTest">
                <methods>
                    <include name="addItem"/>
                    <include name="itemVerification"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>